Algoritmo "saida03"

Var
   idade:inteiro
   salario:real
   nome:caractere
Inicio
      idade <- 32
      salario <- 4560.90
      nome <- "Maria Silva"
      
      //mesclagem de textos e variáveis
      escreval("A funcionaria ", nome, " ganha ", salario:4:2, " e tem ", idade, "anos.")


Fimalgoritmo